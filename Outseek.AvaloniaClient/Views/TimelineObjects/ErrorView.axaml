<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:timelineObjects="clr-namespace:Outseek.AvaloniaClient.ViewModels.TimelineObjects"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Outseek.AvaloniaClient.Views.TimelineObjects.ErrorView">
    <Design.DataContext>
        <timelineObjects:ErrorViewModel/>
    </Design.DataContext>

    <Border Background="#aa0000">
        <Label Content="{Binding Error}" />
    </Border>
</UserControl>
